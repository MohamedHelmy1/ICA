<body>
    <div class="container">
        


    
   
          <div id="right-panel" class="right-panel">
        <!-- Header-->
        <header id="header" class="header">
            <div class="top-left">
                <div class="navbar-header">
                    <h2>@data1.NameOfUser</h2>
                </div>
            </div>
            <div class="top-right">
                <div class="header-menu">
                    <div class="header-left">

                       
                    </div>

                    <div class="user-area dropdown float-right">
                        

                            <a class="nav-link" href="/Home/LoginOut"><i class="fa fa-power-off"></i>Logout</a>
                       
                    </div>
                </div>
            </div>
        </header>
        </div>
        <!-- Footer -->
        <!-- /.site-footer -->
    

        <br /><br />
        <br />
      <!-- Widgets  -->
                <div class="row">
                   

                 <div class="col-lg-6 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <div class="stat-widget-five">
                                    <div class="stat-icon dib flat-color-4">
                                        <i class="pe-7s-users"></i>
                                    </div>
                                    <div class="stat-content">
                                        <div class="text-left dib">
                                            <div class="stat-text"><span class=""> @data1.NameOfUser  -Id </span></div>
                                            <div class="stat-heading">@data1.Id</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-6 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <div class="stat-widget-five">
                                    <div class="stat-icon dib flat-color-4">
                                        <i class="pe-7s-users"></i>
                                    </div>
                                    <div class="stat-content">
                                        <div class="text-left dib">
                                            <div class="stat-text"><span class="count">@data.Count</span></div>
                                            <div class="stat-heading">Marketer Client</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class=" col-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="stat-widget-five">
                                    <div class="stat-icon dib flat-color-4">
                                        <i class="pe-7s-users"></i>
                                    </div>
                                    <div class="stat-content">
                                        <div class="text-left dib">
                                            <div class="stat-text"><span class="">Share Link</span></div>
                                            <input style="width: 100%;border: none;" id="myInput" class="stat-heading" value="@ViewBag.url &&FK_MarketerId=@data1.Id"/>
                                            <button class="btn btn-primary" onclick="myFunction()">copy</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                 <div class="content">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header" style=" display: flex; justify-content: space-between;">
                            <strong class="card-title">All Student Table</strong>
                           
                        </div>
                        <div class="card-body">
                            <table id="tblSlider"  class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        
                                        <th>#</th>
                                        <th>Email</th>
                                       <th>Name</th>
                                       <th>phone</th>
                                       <th>Countery</th>
                                       <th>Date</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in data)
                                   {
                                       <tr>
                                        <th>@Number</th>
                                        <th>@item.Email</th>
                                        <th>@item.name</th>
                                         <th>@item.phone</th>
                                        <th>@item.Cuntery</th>
                                       <th>@item.Date</th>

                                     
                                    </tr>
                                    Number++;
                                   }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- .animated -->
    </div><!-- .content -->
 

<script src="~/js/jquery/jquery.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    
    <!-- drop down & menu -->
